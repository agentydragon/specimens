{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "check": "svelte-check --tsconfig ./tsconfig.app.json",
    "lint": "eslint src --ext .ts,.svelte",
    "lint:fix": "eslint src --ext .ts,.svelte --fix",
    "format": "prettier --write 'src/**/*.{ts,svelte,css}'",
    "format:check": "prettier --check 'src/**/*.{ts,svelte,css}'",
    "codegen": "adgn-generate-frontend-code",
    "prebuild": "npm run codegen"
  },
  "dependencies": {
    "diff": "^5.2.0",
    "dompurify": "^3.0.9",
    "highlight.js": "^11.10.0",
    "json-formatter-js": "^2.5.1",
    "marked": "^12.0.2",
    "marked-highlight": "^2.1.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.1",
    "@sveltejs/vite-plugin-svelte": "^6.1.1",
    "@tsconfig/svelte": "^5.0.4",
    "@types/node": "^22.7.4",
    "@typescript-eslint/eslint-plugin": "^8.18.3",
    "@typescript-eslint/parser": "^8.18.3",
    "eslint": "^9.18.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-svelte": "^3.13.0",
    "jsdom": "^25.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.2",
    "svelte": "^5.38.1",
    "svelte-check": "^4.3.1",
    "svelte-eslint-parser": "^1.4.0",
    "typescript": "~5.8.3",
    "vite": "^7.1.2",
    "vitest": "^2.1.1",
    "json-schema-to-typescript": "^15.0.4"
  }
