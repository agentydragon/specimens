{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "prompt": {
      "description": "The prompt to run on the fetched content",
      "type": "string"
    },
    "url": {
      "description": "The URL to fetch content from",
      "format": "uri",
      "type": "string"
    }
  },
  "required": ["url", "prompt"],
  "type": "object"
}
